{
  "name": "scrcpyx-app",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "DISABLE_SVG=trueexpo run:android --all-arch false",
    "android-release": "expo run:android --variant release",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "init-starter": "node init-starter.js",
    "init-starter-reverse": "node init-starter-reverse.js",
    "proto": "buf generate",
    "proto2": "grpc_tools_node_protoc --ts_proto_out=./compiled_proto  --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false  --proto_path=./proto  ./proto/example/grpc/v1/echo.proto",
    "p3": "grpc_tools_node_protoc --plugin='protoc-gen-ts=./node_modules/.bin/protoc-gen-ts' --ts_out=service=grpc-web:./gen/ts  --proto_path=./proto  ./proto/example/grpc/v1/echo.proto",
    "p4": "grpc_tools_node_protoc --plugin='protoc-gen-ts=./node_modules/.bin/protoc-gen-ts' --ts_out=service=grpc-web:.src/gen  --proto_path=./proto  ./proto/example/grpc/v1/echo.proto"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.13.0",
